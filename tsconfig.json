{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./components/*"],
      "@cache/*": ["./cache/*"],
      "@middleware": ["./middlewares/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/Nav.tsx", "components/Add.tsx", "../jujuba-website/components/LoginButton.tsx", "components/404.tsx", "../jujuba-website/components/AuthCoponent.tsx", "../jujuba-website/pages/not-found.tsx"],
  "exclude": ["node_modules"]
}
